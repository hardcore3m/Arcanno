<!DOCTYPE html>
<html lang="pt-br">

<head>
    <title><%= title %></title>
    <%- include('../partials/head'); %>
</head>

<body>

    <header>
        <%- include('../partials/navbar'); %>
    </header>

    <main>
        <div class="content active">
            <ul class="content-list">
                <% planets.forEach(function(planet){ %>
                <li><a href="/planets/<%= planet.id %>"><img src="/images/planets/<%=planet.id %>.png" alt=""> <%=planet.name.pt %></a></li>
                <% }); %>
            </ul>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>
<%- include('../partials/scripts'); %>

</html>